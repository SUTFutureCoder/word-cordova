<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.scdc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>毒物数据库</name>
    <description>
SCDC毒物数据库
    </description>
    <author email="FutureCoder@aliyun.com" href="project256.com">
        FutureCoder
    </author>
    <content src="https://www.baidu.com" />
    <access origin="*" />
    <allow-navigation href="https://www.baidu.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <icon density="xxhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <icon density="xxxhdpi" src="res/icon/android/icon-96-xhdpi.png" />
        <splash density="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
        <splash density="land-ldpi" src="res/screen/android/screen-ldpi-landscape.png" />
        <splash density="land-mdpi" src="res/screen/android/screen-mdpi-landscape.png" />
        <splash density="land-xhdpi" src="res/screen/android/screen-xhdpi-landscape.png" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash density="port-ldpi" src="res/screen/android/screen-ldpi-portrait.png" />
        <splash density="port-mdpi" src="res/screen/android/screen-mdpi-portrait.png" />
        <splash density="port-xhdpi" src="res/screen/android/screen-xhdpi-portrait.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icon/ios/icon-72-2x.png" width="180" />
        <icon height="60" src="res/icon/ios/icon-57.png" width="60" />
        <icon height="120" src="res/icon/ios/icon-57-2x.png" width="120" />
        <icon height="76" src="res/icon/ios/icon-72.png" width="76" />
        <icon height="152" src="res/icon/ios/icon-72-2x.png" width="152" />
        <icon height="40" src="res/icon/ios/icon-57.png" width="40" />
        <icon height="80" src="res/icon/ios/icon-57-2x.png" width="80" />
        <icon height="57" src="res/icon/ios/icon-57.png" width="57" />
        <icon height="114" src="res/icon/ios/icon-57-2x.png" width="114" />
        <icon height="72" src="res/icon/ios/icon-72-2x.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72-2x.png" width="144" />
        <icon height="167" src="res/icon/ios/icon-72-2x.png" width="167" />
        <icon height="29" src="res/icon/ios/icon-57.png" width="29" />
        <icon height="58" src="res/icon/ios/icon-57-2x.png" width="58" />
        <icon height="50" src="res/icon/ios/icon-57.png" width="50" />
        <icon height="100" src="res/icon/ios/icon-57-2x.png" width="100" />
        <icon height="167" src="res/icon/ios/icon-72-2x.png" width="167" />
        <splash height="480" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
        <splash height="960" src="res/screen/ios/screen-iphone-portrait.png" width="640" />
        <splash height="1024" src="res/screen/ios/screen-ipad-portrait.png" width="768" />
        <splash height="2048" src="res/screen/ios/screen-ipad-portrait-2x.png" width="1536" />
        <splash height="768" src="res/screen/ios/screen-ipad-landscape.png" width="1024" />
        <splash height="1536" src="res/screen/ios/screen-ipad-landscape-2x.png" width="2048" />
        <splash height="1136" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
        <splash height="1334" src="res/screen/ios/screen-iphone-portrait-2x.png" width="750" />
        <splash height="2208" src="res/screen/ios/screen-iphone-portrait-2x.png" width="1242" />
        <splash height="1242" src="res/screen/ios/screen-iphone-landscape-2x.png" width="2208" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <engine name="android" spec="^7.1.0" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-app-update" spec="^1.6.0" />
</widget>
